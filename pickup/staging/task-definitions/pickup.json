[
  {
    "name": "pickup",
    "image": "624304543898.dkr.ecr.eu-central-1.amazonaws.com/pickup",
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8080,
        "protocol": "tcp"
      }
    ],
    "environment": [
      {
        "name": "PICKUP_JDBC_URL",
        "value": "${jdbc_address}"
      },
      {
        "name": "PICKUP_DB_USER",
        "value": "REG_admin"
      }
    ],
    "secrets": [
      {
        "name": "PICKUP_DB_PASSWORD",
        "valueFrom": "arn:aws:ssm:eu-central-1:624304543898:parameter/staging/pickup/db_pass"
      }
    ],
    "memory": 512,
    "cpu": 256,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-create-group": "true",
        "awslogs-group": "pickup-staging",
        "awslogs-region": "eu-central-1",
        "awslogs-stream-prefix": "pickup"
      }
    },
    "mountPoints": [],
    "volumesFrom": []
  }
]