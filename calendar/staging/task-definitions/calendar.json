[{
  "name": "calendar",
  "image": "624304543898.dkr.ecr.eu-central-1.amazonaws.com/calendar",
  "essential": true,
  "portMappings": [{
    "containerPort": 8080,
    "hostPort": 8080,
    "protocol": "tcp"
  }],
  "environment": [
    {
      "name": "CALENDAR_JDBC_URL",
      "value": "${jdbc_address}"
    },
    {
      "name": "CALENDAR_DB_USER",
      "value": "REG_admin"
    },
    {
      "name": "CALENDAR_MQ_USERNAME",
      "value": "calendar"
    },
    {
      "name": "CALENDAR_MQ_CONNECTION_URL",
      "value": "${mq_url}"
    }
  ],
  "secrets":[
    {
      "name": "CALENDAR_DB_PASSWORD",
      "valueFrom": "arn:aws:ssm:eu-central-1:624304543898:parameter/staging/calendar/db_pass"
    },
    {
      "name": "CALENDAR_MQ_PASSWORD",
      "valueFrom": "arn:aws:ssm:eu-central-1:624304543898:parameter/staging/mq/calendar_pass"
    }
  ],
  "memory": 512,
  "cpu": 256,
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-create-group": "true",
      "awslogs-group": "calendar-staging",
      "awslogs-region": "eu-central-1",
      "awslogs-stream-prefix": "calendar"
    }
  },
  "mountPoints": [],
  "volumesFrom": []
}]
