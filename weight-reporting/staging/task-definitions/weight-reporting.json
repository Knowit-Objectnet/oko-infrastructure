[
    {
      "name": "weight-reporting",
      "image": "624304543898.dkr.ecr.eu-central-1.amazonaws.com/weight-reporting",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "WEIGHT_REPORTING_JDBC_URL",
          "value": "${jdbc_address}"
        },
        {
          "name": "WEIGHT_REPORTING_DB_USER",
          "value": "REG_admin"
        }
      ],
      "secrets": [
        {
          "name": "WEIGHT_REPORTING_DB_PASSWORD",
          "valueFrom": "arn:aws:ssm:eu-central-1:624304543898:parameter/staging/weight_reporting/db_pass"
        }
      ],
      "memory": 512,
      "cpu": 256,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "weight-reporting-staging",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "weight-reporting"
        }
      },
      "mountPoints": [],
      "volumesFrom": []
    }
  ]