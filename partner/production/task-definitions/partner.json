[{
    "name": "partner",
    "image": "624304543898.dkr.ecr.eu-central-1.amazonaws.com/partner",
    "essential": true,
    "portMappings": [{
      "containerPort": 8080,
      "hostPort": 8080,
      "protocol": "tcp"
    }],
    "environment": [
      {
        "name": "PARTNER_JDBC_URL",
        "value": "${jdbc_address}"
      },
      {
        "name": "PARTNER_DB_USER",
        "value": "REG_admin"
      }
    ],
    "secrets":[
      {
        "name": "PARTNER_DB_PASSWORD",
        "valueFrom": "arn:aws:ssm:eu-central-1:624304543898:parameter/production/partner/db_pass"
      }
    ],
    "memory": 512,
    "cpu": 256,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-create-group": "true",
        "awslogs-group": "partner-production",
        "awslogs-region": "eu-central-1",
        "awslogs-stream-prefix": "partner"
      }
    },
    "mountPoints": [],
    "volumesFrom": []
  }]
  